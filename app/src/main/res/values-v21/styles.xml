<resources>
    <style name="MainTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/colorPrimaryInternal</item>
        <item name="colorPrimaryDark">@color/colorPrimaryInternalDark</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:statusBarColor">@color/colorPrimaryInternalDark</item>
        <item name="actionBarTheme">@style/ActionBar</item>
    </style>

    <style name="LoginTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/colorPrimary</item>
    </style>
</resources>
